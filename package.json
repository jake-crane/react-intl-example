{
  "name": "react-intl-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p webpack-serve watch-langs",
    "webpack-serve": "webpack serve --mode development",
    "build": "webpack --mode production",
    "extract": "formatjs extract src/**/*.{ts,tsx} --out-file temp.json",
    "compile": "formatjs compile temp.json --out-file lang/en.json",
    "extract-and-compile": "run-s extract compile && rimraf temp.json",
    "manage-translations": "node scripts/manageTranslations.js",
    "build-langs": "run-s extract-and-compile manage-translations",
    "watch-langs": "watch \"npm run build-langs\" src --wait=20 --interval=10"
  },
  "keywords": [],
  "author": "Jake Crane",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@formatjs/cli": "^4.8.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-plugin-formatjs": "^2.20.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.2",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "watch": "^1.0.2",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
